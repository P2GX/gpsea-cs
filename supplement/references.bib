  country         = {Denmark},
  doi             = {10.1111/cge.12952},
  issn-linking    = {0009-9163},
  issue           = {5},
  keywords        = {Animals; Disease Models, Animal; Genetic Association Studies; Humans; Polydactyly, embryology, genetics; Syndrome; Zinc Finger Protein Gli3, genetics; GLI3; Greig cephalopolysyndactyly; Pallister-Hall; polydactyly},
  nlm-id          = {0253664},

  issn-linking    = {1098-3600},
  issue           = {7},
  keywords        = {Cohort Studies; Fibrillin-1, genetics; Fibrillins; Genetic Association Studies; Genotype; Humans; Marfan Syndrome, genetics; Microfilament Proteins, genetics; Mutation; Phenotype},

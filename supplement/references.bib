@Article{PMID_21358634,
  author          = {Goudie, David R. and D'Alessandro, Mariella and Merriman, Barry and Lee, Hane and Szeverényi, Ildikó and Avery, Stuart and O'Connor, Brian D. and Nelson, Stanley F. and Coats, Stephanie E. and Stewart, Arlene and Christie, Lesley and Pichert, Gabriella and Friedel, Jean and Hayes, Ian and Burrows, Nigel and Whittaker, Sean and Gerdes, Anne-Marie and Broesby-Olsen, Sigurd and Ferguson-Smith, Malcolm A. and Verma, Chandra and Lunny, Declan P. and Reversade, Bruno and Lane, E. Birgitte},
